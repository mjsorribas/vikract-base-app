{
  "name": "vikract",
  "version": "0.1.0",
  "private": true,
  "author": "Maximiliano Sorribas",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "serve": "vite preview",
    "lint": "eslint --config eslint.config.mjs .",
    "lint:fix": "eslint src --fix && yarn format",
    "type-check": "tsc --noEmit",
    "format": "prettier --write src",
    "up": "yarn upgrade-interactive",
    "up-latest": "yarn up --latest",
    "release": "standard-version",
    "push-release": "git push --follow-tags origin main",
    "prepare": "husky install",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@chakra-ui/react": "^2.9.0",
    "@contentful/rich-text-react-renderer": "^16.0.1",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.13.0",
    "@fontsource/plus-jakarta-sans": "^5.2.5",
    "@types/react-slick": "^0.23.13",
    "@types/react-test-renderer": "^18.3.0",
    "contentful": "^11.5.7",
    "framer-motion": "^11.3.30",
    "html-react-parser": "^5.2.2",
    "react": "^18.3.1",
    "react-dom": "^19.0.0",
    "react-helmet": "^6.1.0",
    "react-icons": "5.3.0",
    "react-router-dom": "^6.26.1",
    "react-slick": "^0.30.3",
    "react-test-renderer": "^18.3.1",
    "slick-carousel": "^1.8.1",
    "victory": "^37.3.6",
    "vitest": "^2.1.3"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^19.8.0",
    "@eslint/eslintrc": "^3.3.0",
    "@eslint/js": "^9.22.0",
    "@stylistic/eslint-plugin": "^4.2.0",
    "@stylistic/eslint-plugin-ts": "^4.2.0",
    "@types/node": "^22.13.10",
    "@types/react": "^18.3.11",
    "@types/react-dom": "^19.0.4",
    "@types/react-helmet": "^6.1.11",
    "@typescript-eslint/eslint-plugin": "^8.26.1",
    "@typescript-eslint/parser": "^8.26.1",
    "@typescript-eslint/typescript-estree": "^8.26.1",
    "@vitejs/plugin-react": "^4.3.3",
    "@vitest/coverage-v8": "^2.1.3",
    "@vitest/ui": "^2.1.3",
    "commitlint": "^19.8.0",
    "eslint": "^9.22.0",
    "eslint-config-sznm": "^2.0.4",
    "eslint-plugin-import": "^2.31.0",
    "happy-dom": "15.7.4",
    "husky": "^9.1.7",
    "lint-staged": "^15.5.0",
    "prettier": "^3.3.3",
    "standard-version": "^9.5.0",
    "typescript": "^5.6.3",
    "vite": "^5.4.9"
  },
  "engines": {
    "npm": ">=10.0.0 <=11.0.0",
    "node": ">=18.20.0 || >=20.12.2 || >=22.14.0"
  }
}
