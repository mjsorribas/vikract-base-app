    "framer-motion": "^11.2.13",
    "html-react-parser": "^5.1.12",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-helmet": "^6.1.0",
    "react-icons": "^5.2.1",
    "react-router-dom": "^6.24.1",
    "react-slick": "^0.30.2",
