{
  "name": "vikract",
  "version": "0.1.0",
  "private": true,
  "author": "Maximiliano Sorribas",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build && yarn generate-sitemap",
    "serve": "vite preview",
    "lint": "eslint --config eslint.config.mjs .",
    "lint:fix": "eslint src --fix && yarn format",
    "type-check": "tsc --noEmit",
    "format": "prettier --write src",
    "up": "yarn upgrade-interactive",
    "up-latest": "yarn up --latest",
    "release": "standard-version",
    "push-release": "git push --follow-tags origin main",
    "prepare": "husky install",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "coverage": "vitest run --coverage",
    "generate-sitemap": "tsx sitemap-generator.tsx"
  },
  "dependencies": {
    "@analytics/google-analytics": "^1.1.0",
    "@chakra-ui/react": "^2.9.0",
    "@contentful/rich-text-react-renderer": "^16.0.1",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.13.0",
    "@fontsource/plus-jakarta-sans": "^5.2.6",
    "@microsoft/clarity": "^1.0.0",
    "@types/react-slick": "^0.23.13",
    "@types/react-test-renderer": "^19.1.0",
    "@vercel/analytics": "^1.5.0",
    "analytics": "^0.8.16",
    "contentful": "^11.7.4",
    "framer-motion": "^12.12.2",
    "html-react-parser": "^5.2.6",
    "react": "^19.1.1",
    "react-dom": "^19.1.0",
    "react-helmet": "^6.1.0",
    "react-icons": "5.3.0",
    "react-router-dom": "^7.8.2",
    "react-slick": "^0.31.0",
    "react-test-renderer": "^19.1.0",
    "slick-carousel": "^1.8.1",
    "victory": "^37.3.6",
    "vitest": "^3.1.3"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^19.8.1",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.34.0",
    "@stylistic/eslint-plugin": "^4.4.1",
    "@stylistic/eslint-plugin-ts": "^4.4.1",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@types/node": "^24.3.0",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.6",
    "@types/react-helmet": "^6.1.11",
    "@typescript-eslint/eslint-plugin": "^8.34.0",
    "@typescript-eslint/parser": "^8.30.1",
    "@typescript-eslint/typescript-estree": "^8.35.0",
    "@vitejs/plugin-react": "^5.0.4",
    "@vitest/coverage-v8": "^4.0.3",
    "@vitest/ui": "^3.0.9",
    "commitlint": "^19.8.1",
    "dotenv": "^16.5.0",
    "eslint": "^9.28.0",
    "eslint-plugin-import": "^2.31.0",
    "happy-dom": "18.0.1",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.2",
    "next-sitemap": "^4.2.3",
    "prettier": "^3.3.3",
    "standard-version": "^9.5.0",
    "tsx": "^4.20.6",
    "typescript": "^5.8.3",
    "vite": "^5.4.9"
  },
  "engines": {
    "npm": ">=10.0.0 <=11.0.0",
    "node": ">=20.12.2 || >=22.14.0"
  }
}
